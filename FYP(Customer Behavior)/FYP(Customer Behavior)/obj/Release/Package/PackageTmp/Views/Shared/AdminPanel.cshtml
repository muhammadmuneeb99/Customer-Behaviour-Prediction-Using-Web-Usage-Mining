<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <title>emartPanel</title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <script src="~/Scripts/jquery-3.3.1.min.js"></script>
    <link rel="stylesheet" type="text/css" href="~/Content/Roboto.css" />
    <link href="~/Content/jquery.dataTables.min.css" rel="stylesheet" />
    <link href="~/Content/fixedHeader.dataTables.min.css" rel="stylesheet" />
    <script src="~/Scripts/jquery.dataTables.min.js"></script>
    <script src="~/Scripts/dataTables.fixedHeader.min.js"></script>
    <script src="~/Scripts/materialize.js"></script>
    <link href="~/Content/materialize.css" rel="stylesheet" />
    <link rel="stylesheet" href="~/Content/AdminPanel.css">
    <script src="~/Scripts/canvasjs.min.js"></script>
    <script src="~/Scripts/toastr.min.js"></script>
    <link href="~/Content/toastr.min.css" rel="stylesheet" />

    <style>
        nav li:hover {
            background-color: crimson
        }

        @@font-face {
            font-family: 'Tangerine';
            font-style: normal;
            font-weight: 500;
            src: local('Tangerine Regular'), local('Tangerine-Regular'), url('../../Content/IurY6Y5j_oScZZow4VOxCZZM - Copy.woff2') format('woff2');
            unicode-range: U+0000-00FF, U+0131, U+0152-0153, U+02BB-02BC, U+02C6, U+02DA, U+02DC, U+2000-206F, U+2074, U+20AC, U+2122, U+2191, U+2193, U+2212, U+2215, U+FEFF, U+FFFD;
        }
    </style>
</head>

<body class="white">
    <ul id="slide-out" style="background-color:#292929;" class="sidenav">
        <li class="" style="padding-left:30px;">
            <div class="white-text" style="height: 64px; background-color:#292929;">
                <div class="row">
                    <p style="margin-top: 0%;">
                        <a href="" class="white-text" style=" font-family:'Tangerine'; font-size:100px; padding:0%">emart</a>
                    </p>
                </div>
            </div>
        </li>

        <li id="dash_dashboard"><a class="waves-effect" style="color:white;padding-left:15px;  border-top:1px solid #636161;" href='@Url.Action("Index", "Admin")'>Dashboard</a></li>

        <ul class="collapsible ">
            <li id="dash_users">
                <div id="dash_users_header" style="color:white;  border-top:1px solid #636161;" class="collapsible-header waves-effect">Users</div>
                <div id="dash_users_body" class="collapsible-body">
                    <ul>
                        <li id="users_seller" style="background-color:#292929;">
                            <a class="waves-effect" style="text-decoration: none; color:white;" href='@Url.Action("Seller_Info", "Admin")'>Seller</a>
                        </li>
                        <li id="users_customer" style="background-color:#292929;">
                            <a class="waves-effect" style="text-decoration: none; color:white;" href='@Url.Action("Customer", "Admin")'>Customer</a>
                        </li>
                    </ul>
                </div>
            </li>

            <li id="dash_products">
                <div id="dash_products_header" style="color:white;  border-top:1px solid #636161;" class="collapsible-header waves-effect">Products</div>
                <div id="dash_products_body" class="collapsible-body">
                    <ul>
                        <li id="products_product" style="background-color:#292929;">
                            <a class="waves-effect" style="text-decoration: none; color:white;" href='@Url.Action("Product", "Admin")'>Products</a>
                            <a class="waves-effect" style="text-decoration: none; color:white;" href='@Url.Action("Order", "Admin")'>Orders</a>
                        </li>
                    </ul>
                </div>
            </li>

            <li id="dash_categories">
                <div id="dash_categories_header" style="color:whitesmoke;  border-top:1px solid #636161;" class="collapsible-header waves-effect">Categories</div>
                <div id="dash_categories_body" class="collapsible-body">
                    <ul style="background-color:#292929;">
                        <li id="categories_category">
                            <a class="waves-effect" style="text-decoration: none; color:white;" href='@Url.Action("Category", "Admin")'>Category</a>
                        </li>

                        <li id="categories_sub_category">
                            <a class="waves-effect" style="text-decoration: none; color:white;" href='@Url.Action("SubCategory", "Admin")'>Sub Category</a>
                        </li>
                        <li id="categories_sub_category">
                            <a class="waves-effect" style="text-decoration: none; color:white;" href='@Url.Action("SubSubCategory", "Admin")'>Sub Sub Category</a>
                        </li>
                    </ul>
                </div>
            </li>

            <li id="dash_brands">
                <div id="dash_brands_header" style="color:white;  border-bottom:1px solid #636161;  border-top:1px solid #636161;" class="collapsible-header waves-effect">Brands</div>
                <div id="dash_brands_body" class="collapsible-body">
                    <ul style="background-color:#292929;">
                        <li id="brands_brand" style="border-bottom:1px solid #636161;">
                            <a class="waves-effect" style="text-decoration: none; color:whitesmoke;" href='@Url.Action("Brands", "Admin")'>Brands</a>
                        </li>
                    </ul>
                </div>
            </li>
        </ul>
    </ul>

    <header class="stiky-header">
        <ul class="dropdown-content" id="user_dropdown">
            <li><a style="color:#292929;" href='@Url.Action("Logout", "Admin")'>Logout</a></li>
        </ul>
        <nav class="" style="background-color:#292929; padding:0px 10px;">
            <div class="nav-wrapper">
                <a href="#" data-activates="slide-out" data-target="slide-out" class="show-on-large button-collapse sidenav-trigger"><i style="font-size:22px;color:crimson" class="material-icons">menu</i></a>
                <text style="padding-left:20px;">emart Panel</text>
                <ul class="right hide-on-med-and-down">
                    <li>
                        <a class="show-on-large" href="@Url.Action("Index", "Admin")">Home</a>
                    </li>
                    <li>
                        <a class="show-on-large" href="@Url.Action("Seller_Info", "Admin")">Seller</a>
                    </li>
                    <li>
                        <a class="show-on-large" href="@Url.Action("Customer", "Admin")">Customer</a>
                    </li>
                    <li>
                        <a class="show-on-large" href="@Url.Action("Product", "Admin")">Product</a>
                    </li>
                    <li>
                        <a class="show-on-large" href="@Url.Action("Order", "Admin")">Order</a>
                    </li>
                    <li>
                        <a class="show-on-large" href="@Url.Action("Category", "Admin")">Category 1</a>
                    </li>
                    <li>
                        <a class="show-on-large" href="@Url.Action("SubCategory", "Admin")">Category 2</a>
                    </li>
                    <li>
                        <a class="show-on-large" href="@Url.Action("SubSubCategory", "Admin")">Category 3</a>
                    </li>
                    <li>
                        <a class="show-on-large" href="@Url.Action("Brands", "Admin")">Brands</a>
                    </li>
                    <li>
                        <a class="show-on-large" href="@Url.Action("InsertBrandInCat", "Admin")">Brands Category</a>
                    </li>
                    <li>
                        <a class="show-on-large" href="@Url.Action("AnalyticsAdmin", "Admin")">Analytics</a>
                    </li>
                    <li>
                        <a class='right dropdown-trigger' href='#' data-target='user_dropdown'><i class='material-icons'>account_circle</i></a>
                    </li>

                </ul>
            </div>
        </nav>
        @*<nav>
                <div class="nav-wrapper indigo darken-2">
                    <a style="margin-left: 20px;" class="breadcrumb" href="#!">Admin</a>
                    <a class="breadcrumb" href="#!">Index</a>

                    <div style="margin-right: 20px;" id="timestamp" class="right"></div>
                </div>
            </nav>*@
    </header>

    @RenderBody()

</body>
</html>
<script>
    $(document).ready(function () {
        $('select').formSelect();
        $('.sidenav').sidenav();
        $('.collapsible').collapsible();
        $('.dropdown-trigger').dropdown({ coverTrigger: false });
    });
</script>

<script>
    var sessionTimeoutWarning = @Session.Timeout- 1;

    var sTimeout = parseInt(sessionTimeoutWarning) * 60 * 1000;
    setTimeout('SessionEnd()', sTimeout);

    function SessionEnd() {
        window.location = "/Admin/Logout";
    }
</script>
